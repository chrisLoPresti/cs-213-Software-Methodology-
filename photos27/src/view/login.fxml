<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="400.0">
         <children>
            <Button fx:id="login" layoutX="126.0" layoutY="240.0" mnemonicParsing="false" onAction="#login" text="Login " />
            <Button fx:id="clear" layoutX="242.0" layoutY="240.0" mnemonicParsing="false" onAction="#clear" text="Clear" />
            <TextField fx:id="username" layoutX="126.0" layoutY="131.0" promptText="Username" />
            <Label fx:id="loginError" layoutX="71.0" layoutY="84.0" prefHeight="17.0" prefWidth="300.0" textFill="#da0b0b" />
            <Label layoutX="102.0" layoutY="32.0" text="Photo Album Login">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <PasswordField fx:id="password" layoutX="126.0" layoutY="187.0" promptText="Password" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
