            <TextField fx:id="searchUser" layoutX="22.0" layoutY="409.0" prefHeight="27.0" prefWidth="144.0" promptText="Search Username" />
            <Button fx:id="search" layoutX="175.0" layoutY="409.0" mnemonicParsing="false" onAction="#search" text="Search" />
            <Button fx:id="logout" layoutX="510.0" layoutY="409.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
            <Button fx:id="clearNew" layoutX="485.0" layoutY="326.0" mnemonicParsing="false" onAction="#clearNew" text="Clear" />
         </children>
      </AnchorPane>
   </children>
