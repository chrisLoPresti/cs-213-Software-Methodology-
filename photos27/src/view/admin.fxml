            <TextField fx:id="newUserName" layoutX="372.0" layoutY="212.0" promptText="Enter Username" />
            <TextField fx:id="newName" layoutX="372.0" layoutY="249.0" promptText="Enter User's Name" />
            <TextField fx:id="newPassword" layoutX="372.0" layoutY="284.0" promptText="Enter User's Password" />
            <Button fx:id="addUser" layoutX="372.0" layoutY="326.0" mnemonicParsing="false" onAction="#createUser" prefHeight="27.0" prefWidth="106.0" text="Add User" />
            <Label fx:id="selectedUsername" layoutX="425.0" layoutY="78.0" prefHeight="17.0" prefWidth="144.0" />
            <Label layoutX="386.0" layoutY="39.0" text="Selected User" />
            <Label layoutX="23.0" layoutY="31.0" text="User List" />
            <Label fx:id="selectedName" layoutX="429.0" layoutY="121.0" prefHeight="17.0" prefWidth="144.0" />
