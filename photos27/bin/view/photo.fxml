<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PhotoViewController">
   <children>
      <AnchorPane layoutY="1.0" prefHeight="700.0" prefWidth="500.0">
         <children>
            <Button fx:id="logout" layoutX="412.0" layoutY="618.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
            <Button fx:id="addTags" layoutX="95.0" layoutY="39.0" mnemonicParsing="false" onAction="#addTags" text="Add Tags" />
            <ImageView fx:id="myImageView" fitHeight="357.0" fitWidth="412.0" layoutX="44.0" layoutY="84.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="tags" layoutX="46.0" layoutY="471.0" prefHeight="17.0" prefWidth="432.0" text="Tags:" />
            <Button fx:id="next" layoutX="429.0" layoutY="39.0" mnemonicParsing="false" onAction="#next" text="&gt;" />
            <Button fx:id="previous" layoutX="46.0" layoutY="39.0" mnemonicParsing="false" onAction="#previous" text="&lt;" />
            <Button fx:id="removeTags" layoutX="185.0" layoutY="39.0" mnemonicParsing="false" onAction="#removeTags" text="Remove Tags" />
            <Button fx:id="editCaption" layoutX="216.0" layoutY="618.0" mnemonicParsing="false" onAction="#editCaption" text="Edit Caption" />
            <Label layoutX="46.0" layoutY="497.0" prefHeight="17.0" prefWidth="61.0" text="Caption:" />
            <TextArea fx:id="caption" layoutX="41.0" layoutY="522.0" prefHeight="83.0" prefWidth="442.0" />
            <Button fx:id="changeName" layoutX="298.0" layoutY="39.0" mnemonicParsing="false" onAction="#changeName" text="Change Name" />
            <Button fx:id="back" layoutX="43.0" layoutY="618.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
            <Label fx:id="photoName" layoutX="46.0" layoutY="14.0" prefHeight="17.0" prefWidth="204.0" />
            <Label layoutX="46.0" layoutY="448.0" prefHeight="17.0" prefWidth="40.0" text="Date:" />
            <Label fx:id="date" layoutX="87.0" layoutY="448.0" prefHeight="17.0" prefWidth="390.0" />
            <Label fx:id="albumLabel" alignment="CENTER_RIGHT" layoutX="287.0" layoutY="14.0" prefHeight="17.0" prefWidth="173.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
